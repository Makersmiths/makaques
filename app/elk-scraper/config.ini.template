[eventhub]
connection_string = <YOUR_EVENT_HUB_CONNECTION_STRING>
name = <YOUR_EVENT_HUB_NAME>
prefix = 

[azure]
storage_connection_string = <YOUR_STORAGE_ACCOUNT_CONNECTION_STRING>
container_name = <YOUR_CONTAINER_NAME>

[elasticsearch]
host = https://<elastic_ip>:9200
index = makaque
api_key = <YOUR_ELASTIC_API_KEY>
ca_certs = ./ca.crt #Path to CA certificate generated by docker-elk stack. Located at ~/docker-elk/tls/ca/ca.crt
ssl_show_warn = True
verify_certs = False
